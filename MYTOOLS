def pi_real(n):
  strPI_INT = str(PI_INT)
  return '3.' + (strPI_INT[0:n])
print(pi_real(5))
def e_real(n):
  strE_INT = str(E_INT)
  return '2.' + (strE_INT[0:n])
print(e_real(5))